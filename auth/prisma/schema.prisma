generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email    String @unique @db.VarChar(100)
  name     String
  password String @db.VarChar(100)
}
